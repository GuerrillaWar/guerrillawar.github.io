{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jquery": true
  },
  "rules": {
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1,
        "VariableDeclarator": {
          "var": 2,
          "let": 2,
          "const": 3
        }
      }
    ],
    "quotes": [
      2,
      "double"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "comma-dangle": [
      0
    ],
    "no-unused-vars": [
      0
    ],
    "semi": [
      2,
      "never"
    ]
  },
  "globals": {
    "rj": false,
    "Modernizr": false
  },
  "extends": "eslint:recommended"
}
